{
  "model_config": {
    "model_name": "resnet3d_50",
    "num_classes": 14,
    "input_size": [
      64,
      64,
      64
    ],
    "dropout_rate": 0.5,
    "pretrained": true
  },
  "training_config": {
    "batch_size": 8,
    "learning_rate": 0.0001,
    "num_epochs": 100,
    "weight_decay": 1e-05,
    "warmup_epochs": 5,
    "patience": 10,
    "data_dir": "./data",
    "output_dir": "./outputs",
    "checkpoint_dir": "./checkpoints",
    "log_dir": "./logs",
    "device": "auto",
    "num_workers": 4,
    "pin_memory": true,
    "save_every_n_epochs": 5,
    "log_every_n_steps": 10,
    "use_wandb": true,
    "wandb_project": "ct-pathology-detection",
    "export_formats": [
      "pytorch",
      "onnx"
    ]
  },
  "data_config": {
    "ct_rate_dataset_name": "ibrahimhamamci/CT-RATE",
    "train_split": "train",
    "val_split": "validation",
    "normalize_intensity": true,
    "augment_data": true,
    "augmentation_prob": 0.5,
    "dicom_supported_formats": [
      ".dcm",
      ".DCM"
    ],
    "dicom_resize_to": [
      64,
      64,
      64
    ]
  },
  "pathology_labels": [
    "Atelectasis",
    "Cardiomegaly",
    "Consolidation",
    "Edema",
    "Effusion",
    "Emphysema",
    "Fibrosis",
    "Hernia",
    "Infiltration",
    "Mass",
    "Nodule",
    "Pleural_Thickening",
    "Pneumonia",
    "Pneumothorax"
  ]
}